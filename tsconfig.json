{
  "compilerOptions": {
    "target": "ES2022",
  "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
  "skipLibCheck": true,
  "types": ["node"],
  "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist", "node_modules"]
}